        },
        "login": {
            "urls": [
                "http://www.instructables.com/index",
                "http://www.instructables.com/account/login?nxtPgName=Login+%3A&nxtPg=/account/login?nxtPgName=Instructables+-+DIY+How+To+Make+Instructions&nxtPg=/index"
            ],
            "formURL": "http://www.instructables.com/account/login?nxtPgName=Instructables - DIY How To Make Instructions&nxtPg=/index",
            "method": "POST",
            "usernameField": "u",
            "passwordField": "p",
