{
    "*://*.prezi.com/*": {
        "name": "Prezi",
        "logout": {
            "cookies": ["prezi-auth", "auth-sessionid", "sessionid"]
        },
        "login": {
            "urls": ["https://prezi.com/login/"],
            "formURL": "https://prezi.com/api/v2/auth/login/",
            "method": "POST",
            "usernameField": "username",
            "passwordField": "password",
            "check": "a:contains('Logout')"
        }
    }
}