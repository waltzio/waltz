{
    "*://*.pivotaltracker.com/*": {
        "logout": {
            "cookies": ["t_session"]
        },
        "login": {
            "url": "https://www.pivotaltracker.com/signin",
            "method": "POST",
            "usernameField": "credentials[username]",
            "passwordField": "credentials[password]",
            "check": "li.person_name"
        }
    }
}