{
    "*://*.duolingo.com/*": {
        "name": "duoLingo",
        "logout": {
            "cookies": ["duo_main_session", "auth_tkt", "wuuid"]
        },
        "login": {
            "urls": ["https://www.duolingo.com/"],
            "formURL": "http://www.duolingo.com/login",
            "method": "POST",
            "usernameField": "login",
            "passwordField": "password",
            "check": "a#header_userdrop_logout"
        }
    }
}
